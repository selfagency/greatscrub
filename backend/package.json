{
  "name": "great-scrub",
  "version": "0.1.0",
  "description": "Federal website scrubbing monitor",
  "keywords": [
    "government",
    "open-data"
  ],
  "license": "MIT",
  "author": "daniel@self.agency",
  "type": "module",
  "tsup": {
    "cjsInterop": true,
    "dts": true,
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "HTTP_REQUEST_TIMEOUT=5000 node --env-file=.env --enable-source-maps --experimental-specifier-resolution=node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "find-rss": "^1.6.4",
    "p-queue": "^8.1.0",
    "pixelmatch": "^6.0.0",
    "pocketbase": "^0.25.1",
    "puppeteer": "^24.1.1",
    "puppeteer-full-page-screenshot": "^1.2.2",
    "radashi": "^12.3.4",
    "tslog": "^4.9.3",
    "undici": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^4.7.0",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "tslib": "^2.8.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "engines": {
    "node": ">=20.6.0"
  }
}
